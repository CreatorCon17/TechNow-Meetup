        <order>100</order>
        <script><![CDATA[(function() {
	var im = new InviteManager(current.sys_id, gs.getUserID());
	var inviteId = im.userOnList();
	var response = !inviteId ? im.createInvite('Going') : im.updateInvite(inviteId, 'Going');

	if (response == 'Stand by') {
		gs.addErrorMessage('We are at capacity. You are currently on stand by');
	} else {
		gs.addInfoMessage('We look forward to seeing you there');
	}
	action.setRedirectURL(current);

})();]]></script>
